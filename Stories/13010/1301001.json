    {
        "MessageId": 130100040,
        "Flag": 1,
        "Type": 0,
        "NextId": 130100047,
        "MessageType": "Text",
        "ImagePath": "",
        "MessageJP": "先生のご連絡先をもらっておいて正解でした",
