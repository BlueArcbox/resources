    {
        "MessageId": 130100016,
        "Flag": 1,
        "Type": 0,
        "NextId": 130100020,
        "MessageType": "Text",
        "ImagePath": "",
        "MessageJP": "私のこと、覚えてますか？",
        "MessageKR": "저 기억하고 계시죠?",
        "MessageTW": "還記得我嗎？",
        "MessageEN": "Do you remember me?"
    },
    {
        "MessageId": 130100020,
        "Flag": 2,
        "Type": 3,
        "NextId": 130100036,
        "MessageType": "Text",
        "ImagePath": "",
        "MessageJP": "うん、もちろん",
