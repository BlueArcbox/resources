    {
        "MessageId": 200440161,
        "Flag": 2,
        "Type": 0,
        "NextId": 200440170,
        "MessageType": "Text",
        "ImagePath": "",
        "MessageJP": "トレーニングも兼ねられて一石二鳥なんですよ",
