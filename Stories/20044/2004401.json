    {
        "MessageId": 200440110,
        "Flag": 2,
        "Type": 3,
        "NextId": 200440120,
        "MessageType": "Text",
        "ImagePath": "",
        "MessageJP": "そんなに高いの？",
        "MessageKR": "얼마길래?",
        "MessageTW": "要多少錢？",
        "MessageEN": "How much is it?"
    },
