    },
    {
        "MessageId": 200440050,
        "Flag": 1,
        "Type": 0,
        "NextId": 200440051,
        "MessageType": "Text",
        "ImagePath": "",
        "MessageJP": "よろしければ",
        "MessageKR": "이왕 누우신 김에",
